{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": "off",
    "arrow-body-style": "off",
    "react/react-in-jsx-scope": "off",
    "react/function-component-definition": "off",
    "jsx-quotes": "off"
  }
}
